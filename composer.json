		"php-http/message": "^1.0",
		"guzzlehttp/psr7": "^1.0",
		"php-http/guzzle6-adapter": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"DaybreakStudios\\WordpressSDK\\": "src/"
